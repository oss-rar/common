{
  "lerna": "3.4.1",
  "version": "0.0.42-alpha.1",
  "packages": [
    "packages/*"
  ],
  "command": {
    "publish": {
      "npmClient": "npm",
      "conventionalCommits": true,
      "message": "chore(publish): publish %s [skip-ci]"
    },
    "version": {
      "allowBranch": "master",
      "message": "chore(version): version %s [skip-ci]"
    }
  }
}
