  },
  "peerDependencies": {
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.3",
    "pino-pretty": "^4.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {}
      ],
      [
        "@pika/plugin-build-node",
        {}
      ]
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      "@babel/preset-react"
    ],
    "env": {
