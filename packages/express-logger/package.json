{
  "private": false,
  "name": "@oss-rar/express-logger",
  "version": "0.0.17-alpha.0",
  "description": "Express logger",
  "author": "OSS RaR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oss-rar/common.git"
  },
  "scripts": {
